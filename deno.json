{
  "tasks": {
    "run": "deno run -WR src/main.ts",
    "test": "deno test --unstable-raw-imports --parallel src/",
    "test:e2e": "deno test -WRE --allow-run tests/e2e_test.ts",
    "typecheck": "deno check . --unstable-raw-imports --unstable-tsgo",
    "build": "deno -W=bin --unstable-bundle scripts/build-package.ts"
  },
  "fmt": {
    "proseWrap": "preserve"
  },
  "nodeModulesDir": "none",
  "imports": {
    "@david/dax": "jsr:@david/dax@0.43.2",
    "@ryu/enogu": "jsr:@ryu/enogu@0.6.2",
    "@std/assert": "jsr:@std/assert@1.0.15",
    "@std/cli": "jsr:@std/cli@1.0.23",
    "@std/path": "jsr:@std/path@1.1.2",
    "@std/yaml": "jsr:@std/yaml@1.0.10",
    "@std/jsonc": "jsr:@std/jsonc@1.0.2",
    "@valibot/valibot": "jsr:@valibot/valibot@1.1.0"
  },
  "exclude": [
    "testdata/*"
  ]
}
